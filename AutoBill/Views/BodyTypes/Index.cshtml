@model IEnumerable<AutoBill.Models.BodyType>

@{
    ViewData["Title"] = "Index";
}

<h2>@ViewData["Title"]</h2>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
                <th>
                    @Html.DisplayNameFor(model => model.BodyTypeName)
                </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.BodyTypeName)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.BodyTypeId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.BodyTypeId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.BodyTypeId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
